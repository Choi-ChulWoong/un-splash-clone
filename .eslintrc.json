{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "next/core-web-vitals",
    "plugin:@typescript-eslint/recommended",
    "prettier"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint",
    "prettier"
  ],
  "ignorePatterns": [
    "next.config.js"
  ],
  "rules": {
    "@typescript-eslint/no-unused-vars": "warn",
    "@emotion/jsx-import": "error"
  }
}
